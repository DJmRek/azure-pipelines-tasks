{
  "id": "5e3feff0-c5ae-11e8-a7d0-4bd3b8229800",
  "name": "PipAuthenticate",
  "friendlyName": "ms-resource:loc.friendlyName",
  "description": "ms-resource:loc.description",
  "author": "Microsoft Corporation",
  "helpUrl": "https://docs.microsoft.com/azure/devops/pipelines/tasks/package/pip-authenticate",
  "helpMarkDown": "ms-resource:loc.helpMarkDown",
  "category": "Package",
  "version": {
    "Major": 1,
    "Minor": 243,
    "Patch": 28
  },
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "minimumAgentVersion": "2.144.0",
  "instanceNameFormat": "ms-resource:loc.instanceNameFormat",
  "groups": [
    {
      "name": "feedAuthentication",
      "displayName": "ms-resource:loc.group.displayName.feedAuthentication",
      "isExpanded": true
    }
  ],
  "inputs": [
    {
      "name": "workloadIdentityServiceConnection",
      "label": "'Entra Workload ID-backed Azure DevOps user' Service Connection",
      "helpMarkDown": "If this is set, feedUrl is required. All other inputs are ignored.",
      "type": "connectedService:workloadidentityuser",
      "required": false,
      "properties": {
        "EditableOptions": "False",
        "MultiSelectFlatList": "False"
      },
      "groupName": "feedAuthentication"
    },
    {
      "name": "feedUrl",
      "label": "Azure Artifacts Feeds url.",
      "helpMarkDown": "If this is set, workloadIdentityServiceConnection is required. All other inputs are ignored. Not compatible with pythonDownloadServiceConnections.",
      "type": "string",
      "defaultValue": "",
      "required": false,
      "groupName": "feedAuthentication"
    },
    {
      "name": "artifactFeeds",
      "aliases": [
        "artifactFeeds"
      ],
      "type": "pickList",
      "label": "My feeds (select below)",
      "helpMarkDown": "Select feeds to authenticate present in this organization",
      "defaultValue": "",
      "required": "false",
      "groupName": "feedAuthentication",
      "properties": {
        "EditableOptions": "True",
        "MultiSelectFlatList": "True"
      }
    },
    {
      "name": "pythonDownloadServiceConnections",
      "aliases": [
        "pythonDownloadServiceConnections"
      ],
      "type": "connectedService:externalPythonDownloadFeed",
      "label": "Feeds from external organizations",
      "required": false,
      "helpMarkDown": "Select endpoints to authenticate outside this organization.",
      "properties": {
        "EditableOptions": "False",
        "MultiSelectFlatList": "True"
      },
      "groupName": "feedAuthentication"
    },
    {
      "name": "onlyAddExtraIndex",
      "type": "boolean",
      "label": "Don't set primary index URL",
      "defaultValue": false,
      "required": false,
      "helpMarkDown": "If this is set to true, no feed will be set as the primary index URL. All of the configured feeds/endpoints will be set as extra index URLs. Defaults to false.",
      "groupName": "feedAuthentication"
    }
  ],
  "dataSourceBindings": [
    {
      "target": "artifactFeeds",
      "endpointId": "tfs:feed",
      "endpointUrl": "{{endpoint.url}}/_apis/packaging/feedids",
      "resultSelector": "jsonpath:$.value[*]",
      "resultTemplate": "{ \"Value\" : \"{{#if projectName}}{{{projectName}}}\\/{{/if}}{{{name}}}\", \"DisplayValue\" : \"{{{name}}}\" }"
    }
  ],
  "execution": {
    "Node10": {
      "target": "pipauthenticatemain.js",
      "argumentFormat": ""
    },
    "Node16": {
      "target": "pipauthenticatemain.js",
      "argumentFormat": ""
    },
    "Node20_1": {
      "target": "pipauthenticatemain.js",
      "argumentFormat": ""
    }
  },
  "messages": {
    "Info_AddingInternalFeeds": "ms-resource:loc.messages.Info_AddingInternalFeeds",
    "Info_AddingExternalFeeds": "ms-resource:loc.messages.Info_AddingExternalFeeds",
    "Info_AddingFederatedFeedAuth": "ms-resource:loc.messages.Info_AddingFederatedFeedAuth",
    "Info_SuccessAddingAuth": "ms-resource:loc.messages.Info_SuccessAddingAuth",
    "Info_SuccessAddingFederatedFeedAuth": "ms-resource:loc.messages.Info_SuccessAddingFederatedFeedAuth",
    "Info_AddingPasswordAuthEntry": "ms-resource:loc.messages.Info_AddingPasswordAuthEntry",
    "Info_AddingTokenAuthEntry": "ms-resource:loc.messages.Info_AddingTokenAuthEntry",
    "Error_FailedToParseFeedUrlAndAuth": "ms-resource:loc.messages.Error_FailedToParseFeedUrlAndAuth",
    "FailedToGetPackagingUri": "ms-resource:loc.messages.FailedToGetPackagingUri",
    "FailedToGetServiceConnectionAuth": "ms-resource:loc.messages.FailedToGetServiceConnectionAuth",
    "FailedToAddAuthentication": "ms-resource:loc.messages.FailedToAddAuthentication",
    "Warn_TooManyFeedEntries": "ms-resource:loc.messages.Warn_TooManyFeedEntries",
    "Warning_SessionCreationFailed": "ms-resource:loc.messages.Warning_SessionCreationFailed"
  },
  "_buildConfigMapping": {
    "Default": "1.243.0",
    "Node20_229_1": "1.243.29",
    "wif_242": "1.243.28"
  }
}